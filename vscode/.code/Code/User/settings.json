    "editor.wordBasedSuggestions": "off",
    "emmet.showSuggestionsAsSnippets": true,
    "errorLens.borderRadius": "2px",
    "errorLens.editorHoverPartsEnabled": {
        "buttonsEnabled": false,
        "messageEnabled": false,
        "sourceCodeEnabled": false
    },
    "errorLens.enableOnDiffView": true,
    "errorLens.fontWeight": "100",
    "errorLens.gutterIconSet": "default",
    "errorLens.messageBackgroundMode": "message",
    "errorLens.messageMaxChars": 200,
    "errorLens.messageTemplate": "$severity $message",
    "errorLens.removeLinebreaks": false,
    "errorLens.severityText": ["👷🏻", "📝", "🔥", "🚧"],
    "errorLens.statusBarIconsEnabled": true,
    "errorLens.statusBarIconsUseBackground": false,
    "errorLens.statusBarMessageType": "closestSeverity",
    "eslint.alwaysShowStatus": true,
    "eslint.format.enable": true,
    "eslint.lintTask.enable": true,
    "eslint.validate": [
        "astro",
        "javascript",
        "javascriptreact", // Enable .astro
        "typescript", // Enable .ts
        "typescriptreact" // Enable .tsx
    ],
    "explorer.fileNesting.enabled": false,
    "explorer.fileNesting.patterns": {
        "*.cjs": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.cts": "${capture}.typegen.ts",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.mjs": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.mts": "${capture}.typegen.ts",
        "*.ts": "${capture}.js, ${capture}.typegen.ts",
        "*.tsx": "${capture}.ts, ${capture}.typegen.ts",
        "cargo.toml": "cargo.lock",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
        "tsconfig.json": "tsconfig.*.json"
    },
    "files.associations": {
        "*.css": "tailwindcss",
        "*.scss": "tailwindcss",
        "*.ts": "typescript"
    },
    "files.autoGuessEncoding": true,
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
